@model Events.Web.Models.AllParticipantList

@{
    ViewBag.Title = "Participants";
}

<h2>Participants</h2>
<div class="row">
    @if (Model.participants.Any())
    {
        @Html.DisplayFor(x => x.participants)
    }
    else
    {
        <div class="col-md-4 col-sm-6 col-xs-12">No Participants</div>
    }
</div>

@section scripts {
    @Scripts.Render("~/bundles/ajax")
}